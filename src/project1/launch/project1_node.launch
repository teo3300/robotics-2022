<launch>
    <arg name="bag_name" value="bag1"/>
    <node pkg="rosbag" type="play" name="bag1_play" args="--clock $(find project1)/bags/$(arg bag_name).bag"/>
    
    <node pkg="velocity" type="velocity_calculator" name="velocity_calculator"/>

    <!-- test integrator, should not print other things except for setup if velocity node is not running -->
    <node pkg="integrator" type="integrator_node" name="integrator_node" output="screen"/>

    <!-- the purpose of this node is to compare output position based on both RPM and tick, to determine the correct N (number of ticks in a full revolution) -->
    <!-- node pkg="project1" type="vel-tick_test_node" name="vel-tick_test_node" output="screen"/> -->
</launch>